# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Meta/Instagram Configuration
# Get these from: https://developers.facebook.com/apps/
INSTAGRAM_APP_ID=your-meta-app-id
INSTAGRAM_APP_SECRET=your-meta-app-secret

# Webhook Configuration
INSTAGRAM_WEBHOOK_VERIFY_TOKEN=your_custom_verify_token_here

# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# LLM API Keys (Default/Fallback)
GEMINI_API_KEY=your-gemini-api-key

# Optional: Encryption Key for Sensitive Data
ENCRYPTION_KEY=your-32-char-encryption-key

